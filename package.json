{
  "name": "map-factory",
  "version": "1.0.0",
  "description": "Object mapping tool",
  "main": "./dist/lib/index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc --pretty -p ./ -d",
    "clean": "./node_modules/.bin/rimraf dist/*",
    "precover": "npm run build",
    "cover": "istanbul cover ./node_modules/nodeunit/bin/nodeunit ./dist/test",
    "coveralls": "istanbul cover ./node_modules/nodeunit/bin/nodeunit ./dist/test && cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "pretest": "npm run build",
    "test": "nodeunit ./dist/test/",
    "pretravis": "npm run build",
    "travis": "npm run coveralls",
    "premajor": "npm run build",
    "major": "npm version major -m \"published to npm as v%s\" && git push --all && git push --tags && npm publish",
    "preminor": "npm run build",
    "minor": "npm version minor -m \"published to npm as v%s\" && git push --all && git push --tags && npm publish",
    "prepatch": "npm run build",
    "patch": "npm version patch -m \"published to npm as v%s\" && git push --all && git push --tags && npm publish"
  },
  "bugs": {
    "url": "https://github.com/midknight41/map-factory/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/midknight41/map-factory.git"
  },
  "keywords": [
    "object",
    "mapping",
    "mapper",
    "fluent",
    "transform"
  ],
  "author": "Chad Macey <chad_macey@hotmail.com>",
  "contributors": [
    "Chad Macey <chad_macey@hotmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "object-mapper": "^3.0.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "gently": "^0.10.0",
    "istanbul": "^0.4.4",
    "jscoverage": "^0.6.0",
    "nodeunit": "^0.9.1",
    "rimraf": "^2.5.3",
    "typescript": "^1.8.10"
  },
  "files": ["dist"]
}
