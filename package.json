{
  "name": "map-factory",
  "version": "0.2.0",
  "description": "Object mapping tool",
  "main": "./dist/lib/index.js",
  "scripts": {
    "test": "./node_modules/.bin/nodeunit ./dist/test/",
    "cover": "istanbul cover ./node_modules/nodeunit/bin/nodeunit ./dist/test",
    "coveralls": "istanbul cover ./node_modules/nodeunit/bin/nodeunit ./dist/test && cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "major": "npm version major -m \"published to npm as v%s\" && git push --tags && npm publish",
    "minor": "npm version minor -m \"published to npm as v%s\" && git push --tags && npm publish",
    "patch": "npm version patch -m \"published to npm as v%s\" && git push --tags && npm publish"
  },
  "bugs": {
    "url": "https://github.com/midknight41/map-factory/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/midknight41/map-factory.git"
  },
  "keywords": [
    "object",
    "mapping",
    "mapper",
    "fluent",
    "transform"
  ],
  "author": "Chad Macey <chad_macey@hotmail.com>",
  "contributors": [
    "Chad Macey <chad_macey@hotmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "object-mapper": "^3.0.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "gently": "^0.10.0",
    "istanbul": "^0.4.4",
    "jscoverage": "^0.6.0",
    "nodeunit": "^0.9.1"
  }
}
